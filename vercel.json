{
    "version": 2, 
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "server.js"
        }
      ],
    "headers": [
      {
        "source": "/server.js",
        "headers": {
          "key": "CORS_ALLOWED_ORIGINS",
          "value": "true"
        }
      }
    ]
    
  }